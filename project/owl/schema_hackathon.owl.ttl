monet_schema:Dissolving a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "Dissolving" ;
    dcterms:title "Dissolving" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass monet_schema:SolventEnum ;
            owl:onProperty monet_schema:solvent ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:onClass monet_schema:MaterialContainer ;
            owl:onProperty monet_schema:container ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:onClass monet_schema:Shaker ;
            owl:onProperty monet_schema:shaker_selection ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:onClass monet_schema:QuantityValue ;
            owl:onProperty monet_schema:volume ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:onClass monet_schema:NamedThing ;
            owl:onProperty monet_schema:material_output ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:onClass monet_schema:NamedThing ;
            owl:onProperty monet_schema:material_input ;
            owl:qualifiedCardinality 1 ] ;
    skos:altLabel "dissolution-activity" ;
    skos:exactMatch CHMO:0002773 .

