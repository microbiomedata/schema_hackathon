@prefix hackschema: <http://example.com/hackschema/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

hackschema:Human a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:name "eye color" ;
            sh:order 1 ;
            sh:path hackschema:eye_color ],
        [ sh:description "The word, words or phrase that the human's mother uses or used to get the attention of the human" ;
            sh:maxCount 1 ;
            sh:order 0 ;
            sh:path hackschema:human_full_name ] ;
    sh:targetClass hackschema:Human .

hackschema:Mammal a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:maxCount 1 ;
            sh:name "eye color" ;
            sh:order 0 ;
            sh:path hackschema:eye_color ] ;
    sh:targetClass hackschema:Mammal .

